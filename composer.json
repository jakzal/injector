{
    "name": "zalas/injector",
    "description": "Injects services from a PSR-11 service container into objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "psr/container": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpspec/prophecy": "^1.9",
        "zalas/phpunit-doubles": "^1.6",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Zalas\\Injector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalas\\Injector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
